{
  "name": "NeoEarth-MC",
  "version": "1.0.3",
  "publicVersion": "1.0.3",
  "description": "Launcher officiel",
  "main": "Main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "win": "electron-builder --windows",
    "mac": "electron-builder --mac"
  },
  "keywords": [],
  "author": "NeoEarth-MC",
  "license": "ISC",
  "devDependencies": {
    "electron": "^29.1.2",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "discord-rpc": "^4.0.1",
    "dotenv": "^16.4.7",
    "electron-log": "^5.1.2",
    "electron-store": "^8.1.0",
    "express": "^4.18.3",
    "express-handlebars": "^7.1.2",
    "fs": "^0.0.1-security",
    "minecraft-launcher-core": "^3.18.2",
    "nodejs-file-downloader": "^4.13.0",
    "xml-js": "^1.6.11"
  },
"build": {
  "defaultArch": "x64",  
  "win": {
    "icon": "./assets/icon/logo.png",
    "publisherName": "NeoEarth-MC",
    "target": "nsis"
  },
  "mac": {
    "icon": "./assets/icon/logo.png",
    "target": ["dmg", "zip"],
    "category": "public.app-category.games"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true,
    "perMachine": true,
    "runAfterFinish": true,
    "uninstallDisplayName": "NeoEarth-MC",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "NeoEarth"
    }  
  }
}
